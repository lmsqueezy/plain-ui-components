{
    "name": "lemonsqueezy/plain-ui-components",
    "description": "Plain UI Components for Laravel",
    "license": "MIT",
    "homepage": "https://docs.plain.com/adding-context/customer-cards",
    "support": {
        "issues": "https://github.com/lmsqueezy/plain-ui-components/issues",
        "source": "https://github.com/lmsqueezy/plain-ui-components"
    },
    "require": {
        "php": "~8.1.0|~8.2.0",
        "ext-json": "*",
        "illuminate/http": "^10.10",
        "illuminate/support": "^10.10"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "LemonSqueezy\\PlainUiComponents\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LemonSqueezy\\PlainUiComponents\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
